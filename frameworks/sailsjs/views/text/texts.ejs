<!-- Default home page -->
<link href='http://fonts.googleapis.com/css?family=Open+Sans:400italic,600' rel='stylesheet' type='text/css'>
<link href="/styles/material.min.css" />
<link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
<div class="wrap">
    <h1>Texts</h1>
    <div class="container">
        <% users.forEach(function(el, index) { %>
        <div class="mdl-card mdl-shadow--2dp demo-card-wide element">
            <div class="mdl-card__title">
                <h2 class="mdl-card__title-text"><%- el.title %></h2>
            </div>
            <div class="mdl-card__supporting-text">
                <%- el.content %>
            </div>
            <div class="mdl-card__menu">
                <button class="mdl-button mdl-button--icon mdl-js-button mdl-js-ripple-effect">
                    <i class="material-icons">share</i>
                </button>
            </div>
        </div>
        <%});%>
    </div>
</div>